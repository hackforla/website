{
  "env": {
    "browser": true,
      "es2021": true,
      "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "rules": {
      "no-mixed-spaces-and-tabs": "off",
      "no-unused-vars": "off",
      "no-extra-semi": "off",
      "no-prototype-builtins": "off"
    }
}
